{
    "manifest_version": 2,
    "name": "Hide X-out Propperties",
    "version": "1.0",
    "description": "This extension hides all X-out properties on redfin",
    "content_scripts": [
        {
            "matches": [
                "https://*.redfin.com/*"
            ],
            "css": ["main.css"],
            "js": ["main.js"]
        }
    ]
}